<h1>Wikis#index</h1>

<div class="row">
  <div class="col-md-6">
    <% @wikis.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
          </h4>
        </div>
      </div>
    <% end %>
    </div>


  <div class="col-md-2">
    <!-- <% if policy(Wiki).new? %> -->
      <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
    <!-- <% end %> -->
  </div>

</div>
